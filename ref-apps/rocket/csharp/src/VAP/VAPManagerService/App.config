<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--Pipeline Configuration-->
    <!--
    0: Line-based counting
    1: ONNX YOLOv3 on every frame
    2: ONNX YOLOv3Tiny on every frame
    3: BGS early filtering -> YOLOv3
    4: BGS early filtering -> YOLOv3Tiny -> YOLOv3
    
    5: ONNX YOLOv3 detection on every frame (without line)
    -->
    <add key="PplConfig" value="5" />

    <add key="StorageConnectionString" value="" />
    <!--<add key="StorageConnectionString" value="DefaultEndpointsProtocol=https;AccountName=[AccountName];AccountKey=[AccountKey]" />-->
    
    <add key="Runtime" value="docker" /> <!--"docker", "vs"-->
  </appSettings>
</configuration>